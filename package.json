{
  "name": "myapp2",
  "private": true,
  "workspaces": [
    "./apps/*",
    "./packages/*"
  ],
  "scripts": {
    "native": "cd apps/expo && yarn start",
    "watch": "yarn workspaces foreach -pi run watch",
    "vercel:install": "yarn set version 3.5 && yarn install",
    "web": "yarn build && cd apps/next && yarn next",
    "build:web": "yarn build && cd apps/next && yarn build",
    "web:extract": "DISABLE_EXTRACTION=false yarn workspace next-app dev",
    "web:prod": "yarn workspace next-app build",
    "web:prod:serve": "yarn workspace next-app serve",
    "fix": "manypkg fix",
    "postinstall": "yarn check-deps && yarn build",
    "build": "yarn workspaces foreach --all --exclude next-app run build",
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest react-native-web-lite@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary react-native-web-lite@canary",
    "check-deps": "check-dependency-version-consistency ."
  },
  "resolutions": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-refresh": "^0.14.0",
    "react-native-svg": "15.3.0",
    "react-native-web": "~0.19.12"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@manypkg/cli": "^0.19.1",
    "@privy-io/react-auth": "^2.6.1",
    "@privy-io/wagmi": "^1.0.3",
    "@react-google-maps/api": "^2.19.3",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@shopify/flash-list": "1.6.4",
    "@solana/spl-token": "^0.3.9",
    "@solana/web3.js": "^1.87.6",
    "@tanstack/react-query": "^5.67.1",
    "@tanstack/react-virtual": "^3.0.1",
    "@web3icons/react": "^4.0.10",
    "aws-sdk": "^2.1525.0",
    "bignumber.js": "^9.1.2",
    "check-dependency-version-consistency": "^3.0.3",
    "eslint": "^8.21.0",
    "ethers": "^6.9.0",
    "expo-camera": "~15.0.14",
    "expo-clipboard": "~6.0.3",
    "expo-linear-gradient": "~13.0.2",
    "expo-location": "~17.0.1",
    "expo-web-browser": "~13.0.3",
    "html5-qrcode": "^2.3.8",
    "intl-pluralrules": "^2.0.1",
    "jsqr": "^1.4.0",
    "lodash": "^4.17.21",
    "next-pwa": "^5.6.0",
    "node-gyp": "^9.3.1",
    "permissionless": "^0.2.35",
    "prettier": "^2.7.1",
    "qrcode.react": "^3.1.0",
    "react-i18next": "^14.0.0",
    "react-native-barcode-mask": "^1.2.4",
    "react-native-confirmation-code-field": "^7.3.2",
    "react-native-country-picker-modal": "^2.0.0",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-largelist": "^3.1.0-rc.2",
    "react-native-maps": "~1.8.0",
    "react-native-qrcode-svg": "^6.3.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-storage": "^1.0.1",
    "react-photo-view": "^1.2.6",
    "react-share": "^5.2.2",
    "selection-extra": "^0.0.6",
    "swiper": "^11.1.5",
    "turbo": "^1.10.3",
    "typescript": "^5.3.3",
    "viem": "2.x",
    "wagmi": "^2.14.12"
  },
  "packageManager": "yarn@4.0.2",
  "devDependencies": {
    "@types/lodash": "^4",
    "@types/next-pwa": "^5"
  }
}
